<div class="panel panel-default">
  <div class="panel-heading">
    <%#= link_to repo_path(project.repo_params) do %>
      <%= project.name %>
    <%# end %>

    <section class="pull-right">
      <%= project.code %>
    </section>
  </div>

  <div class="panel-body">
    基金类型: <%= project.mold %>
    <br>
    基金公司: <%= link_to project.catalog.name, project.catalog %>
    <%= project.description %>
  </div>

  <div class="panel-footer">
    <%#= render "projects/footer_info", project: project %>
  </div>
</div>
