<p id="notice"><%= notice %></p>

<h1>Quotes</h1>

<table>
  <thead>
    <tr>
      <th>Code</th>
      <th>Name</th>
      <th>Record at</th>
      <th>Catalog</th>
      <th>Price</th>
      <th>Up down number</th>
      <th>Up down rate</th>
      <th>Last updated at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @quotes.each do |quote| %>
      <tr>
        <td><%= quote.code %></td>
        <td><%= quote.name %></td>
        <td><%= quote.record_at %></td>
        <td><%= quote.catalog %></td>
        <td><%= quote.price %></td>
        <td><%= quote.up_down_number %></td>
        <td><%= quote.up_down_rate %></td>
        <td><%= quote.last_updated_at %></td>
        <td><%= link_to 'Show', quote %></td>
        <td><%= link_to 'Edit', edit_quote_path(quote) %></td>
        <td><%= link_to 'Destroy', quote, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Quote', new_quote_path %>
